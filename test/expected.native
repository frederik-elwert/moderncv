Pandoc
  Meta
    { unMeta =
        fromList
          [ ( "address"
            , MetaList
                [ MetaInlines
                    [ Str "456"
                    , Space
                    , Str "Research"
                    , Space
                    , Str "Blvd"
                    ]
                , MetaInlines [ Str "Academic" , Space , Str "City" ]
                , MetaInlines [ Str "AC" , Space , Str "12345" ]
                ]
            )
          , ( "cvcolor" , MetaInlines [ Str "green" ] )
          , ( "documentclass" , MetaString "moderncv" )
          , ( "email"
            , MetaInlines [ Str "jane.smith@university.edu" ]
            )
          , ( "header-includes"
            , MetaList
                [ MetaBlocks
                    [ RawBlock
                        (Format "latex")
                        "\\usepackage[scale=0.75]{geometry}\n\\AtBeginDocument{\\recomputelengths}\n"
                    ]
                , MetaBlocks
                    [ RawBlock
                        (Format "latex") "\\moderncvstyle{classic}"
                    ]
                , MetaBlocks
                    [ RawBlock (Format "latex") "\\moderncvcolor{green}"
                    ]
                , MetaBlocks
                    [ Para
                        [ RawInline (Format "latex") "\\name{"
                        , Emph [ Str "Dr." ]
                        , Space
                        , Str "Jane"
                        , Space
                        , Str "Smith"
                        , RawInline (Format "latex") "}{}"
                        ]
                    ]
                , MetaBlocks
                    [ Para
                        [ RawInline (Format "latex") "\\title{"
                        , Str "Curriculum"
                        , Space
                        , Str "Vitae"
                        , RawInline (Format "latex") "}"
                        ]
                    ]
                , MetaBlocks
                    [ Para
                        [ RawInline (Format "latex") "\\address"
                        , RawInline (Format "latex") "{"
                        , Str "456"
                        , Space
                        , Str "Research"
                        , Space
                        , Str "Blvd"
                        , RawInline (Format "latex") "}"
                        , RawInline (Format "latex") "{"
                        , Str "Academic"
                        , Space
                        , Str "City"
                        , RawInline (Format "latex") "}"
                        , RawInline (Format "latex") "{"
                        , Str "AC"
                        , Space
                        , Str "12345"
                        , RawInline (Format "latex") "}"
                        , RawInline (Format "latex") ""
                        ]
                    ]
                , MetaBlocks
                    [ Para
                        [ RawInline (Format "latex") "\\phone{"
                        , Str "+1"
                        , Space
                        , Str "555"
                        , Space
                        , Str "123"
                        , Space
                        , Str "4567"
                        , RawInline (Format "latex") "}"
                        ]
                    ]
                , MetaBlocks
                    [ Para
                        [ RawInline (Format "latex") "\\email{"
                        , Str "jane.smith@university.edu"
                        , RawInline (Format "latex") "}"
                        ]
                    ]
                , MetaBlocks
                    [ Para
                        [ RawInline (Format "latex") "\\homepage{"
                        , Str "https://janesmith.academia.edu"
                        , RawInline (Format "latex") "}"
                        ]
                    ]
                , MetaBlocks
                    [ Para
                        [ RawInline
                            (Format "latex")
                            "\\social[orcid]{0000-0000-0000-0000}"
                        ]
                    ]
                , MetaBlocks
                    [ Para
                        [ RawInline (Format "latex") "\\photo{"
                        , Str "profile.jpg"
                        , RawInline (Format "latex") "}"
                        ]
                    ]
                ]
            )
          , ( "homepage"
            , MetaInlines [ Str "https://janesmith.academia.edu" ]
            )
          , ( "name"
            , MetaInlines
                [ Emph [ Str "Dr." ]
                , Space
                , Str "Jane"
                , Space
                , Str "Smith"
                ]
            )
          , ( "phone"
            , MetaInlines
                [ Str "+1"
                , Space
                , Str "555"
                , Space
                , Str "123"
                , Space
                , Str "4567"
                ]
            )
          , ( "photo" , MetaInlines [ Str "profile.jpg" ] )
          , ( "social"
            , MetaMap
                (fromList
                   [ ( "orcid"
                     , MetaInlines [ Str "0000-0000-0000-0000" ]
                     )
                   ])
            )
          , ( "title"
            , MetaInlines [ Str "Curriculum" , Space , Str "Vitae" ]
            )
          ]
    }
